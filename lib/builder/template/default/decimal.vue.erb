<!-- <%= "#{@field[:name]}" %> -->
<el-row :gutter='24'>
  <el-col :span='24'>
    <el-form-item label='<%= @field[:label] %>' :label-width='formLabelWidth'>
      <el-input-number
        v-model='this.<%= "#{@options[:model][:name]}.#{@field[:name]}" %>'
        :precision="2"
        :step="0.1"
        :max="10"
        autocomplete='off'></el-input-number>
    </el-form-item>
  </el-col>
</el-row>

